<Window x:Class="WpfTeam04.Project.WpfDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTeam04.Project"
        mc:Ignorable="d"
        Title="WpfDashboard" Height="450" Width="800">
    <Grid>
        <StackPanel Background="Snow" VerticalAlignment="Center">
            <Button x:Name="btnMainWindow"  Width="200" Background="Orange" FontSize="20" Margin="0 10 0 0" Click="btnMainWindow_Click">Start Scherm</Button>
            <Button x:Name="btnWpflogin" FontSize="20" Width="200" Background="Orange" Margin="0 10 0 0" Click="btnWpflogin_Click">Login</Button>
            <Button x:Name="btnWpfSettings" FontSize="20" Visibility="Hidden" Width="200" Background="Orange" Margin="0 10 0 0" Click="btnWpfSettings_Click">Settings</Button>
           
          


        </StackPanel>
    </Grid>
</Window>
